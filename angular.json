{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-cusom-cli": {
      "projectType": "library",
      "root": "projects/ng-custom-cli",
      "sourceRoot": "projects/ng-custom-cli/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-custom-cli/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-custom-cli/tsconfig.json"
            },
            "development": {
              "tsConfig": "projects/ng-custom-cli/tsconfig.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-custom-cli/src/test.ts",
            "tsConfig": "projects/ng-custom-cli/tsconfig.spec.json",
            "karmaConfig": "projects/ng-custom-cli/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "ng-custom-cli"
}
